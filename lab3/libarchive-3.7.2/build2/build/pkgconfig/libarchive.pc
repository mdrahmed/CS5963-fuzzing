prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: libarchive
Description: library that can create and read several streaming archive formats
Version: 3.7.2
Cflags: -I${includedir}
Cflags.private: -DLIBARCHIVE_STATIC
Libs: -L${libdir} -larchive
Libs.private:  -lz -lbz2 -llzma -llz4 -lcrypto -lxml2 -lacl -lssl -L/usr/lib/x86_64-linux-gnu -lxml2 -licui18n -licuuc -licudata -lz -llzma -lm
Requires.private: 
